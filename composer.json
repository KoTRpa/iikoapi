{
    "name": "kotrpa/iikoapi",
    "type": "library",
    "description": "IikoBiz API",
    "keywords": [
        "Iiko", "IikoBiz"
    ],
    "homepage": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Kashkin",
            "email": "4kotrpa@gmail.com",
            "homepage": "https://github.com/kotrpa"
        }
    ],
    "require": {
        "php": ">=7.4.*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7 || ^6.4 || ^7.0",
        "psr/log": "^1.1"
    },
    "suggest": {
        "psr/log": "Required for using the Log middleware"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KMA\\IikoApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KMA\\IikoApi\\Tests\\": "tests/"
        }
    }
}
