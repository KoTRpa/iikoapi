{
    "name": "kma/iikoapi",
    "type": "library",
    "description": "IikoBiz API",
    "keywords": [
        "Iiko", "IikoBiz"
    ],
    "homepage": "https://github.com/kotrpa/iikoapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Maxim Kashkin",
            "email": "4kotrpa@gmail.com",
            "homepage": "https://github.com/kotrpa"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0",
        "netresearch/jsonmapper": "^4.0",
        "vlucas/phpdotenv": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "psr/log": "^1.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "KMA\\IikoApi\\IikoServiceProvider"
            ],
            "aliases": {
                "Iiko": "KMA\\IikoApi\\Facades\\IikoFacade"
            }
        }
    },
    "suggest": {
        "psr/log": "Required for using the Log middleware"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KMA\\IikoApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KMA\\IikoApi\\Tests\\": "tests/"
        }
    }
}
